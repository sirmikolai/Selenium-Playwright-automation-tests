<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="test suite">
    <test name="InitializeEnvironmentTest">
        <classes>
            <class name="testngtests.InitializeEnvironmentTest"/>
        </classes>
    </test>
    <test name="e2e tests" parallel="classes" thread-count="6">
        <classes>
            <class name="testngtests.RegistrationTest"/>
            <class name="testngtests.LoginTest"/>
            <class name="testngtests.AdminPanelTest"/>
            <class name="testngtests.ChangingPasswordTest"/>
            <class name="testngtests.CarBrandTest"/>
            <class name="testngtests.CarModelTest"/>
        </classes>
    </test>
    <test name="RemovingTestData" thread-count="2">
        <classes>
            <class name="testngtests.RemovingUsersTest"/>
            <class name="testngtests.RemovingCarBrandsTest"/>
        </classes>
    </test>
</suite>